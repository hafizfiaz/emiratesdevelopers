  <story>
    <para style="terp_default_8">
      <font color="white"> </font>
    </para>
    <para style="terp_default_8">
      <font color="white"> </font>
    </para>
    <para style="terp_default_8">[[repeatIn(objects,'o')]]</para>
    <para style="terp_default_8">[[ setLang(o.partner_id.lang) ]]</para>
    <blockTable colWidths="265.0,101.0,175.0" style="Table1">
      <tr>
        <td>
          <para style="P2">[[ (o.partner_id and o.partner_id.name) or '' ]]</para>

          <para style="terp_default_9">
            <font color="white"> </font>
          </para>
          <para style="terp_default_9">Mobile. : [[ (o.partner_id.mobile) or removeParentNode('para') ]]</para>
          <para style="terp_default_9">Phone : [[ (o.partner_id.phone) or removeParentNode('para') ]]</para>
          <para style="terp_default_9">Email : [[ (o.partner_id.email) or removeParentNode('para') ]]</para>
          <para style="terp_default_9">
            <font color="white"> </font>
          </para>
        </td>
        <td>
          <para style="terp_default_8">
            <font color="white"> </font>
          </para>
        </td>
        <td>
          <barCode code="code128" x="0.0cm" y="0.0cm" quiet="0" style="barcode" barWidth="1" barHeight="28" fontName="Times-Roman" fontSize="100" alignment="CENTER"> [[ o.sale_id.serial_number or removeParentNode('barCode') ]] </barCode>
          <para style="terp_default_9_Center">[[ o.sale_id.serial_number or '' ]] </para>
        </td>
      </tr>
    </blockTable>
    <para style="terp_default_8">
      <font color="white"> </font>
    </para>
    <para style="terp_header">[[ o.sale_id.state in ['draft','sent','under_gm_review','under_account_review','gm_approved','gm_rejected','under_pm_review','pm_verified','pm_rejected','cancel'] and removeParentNode('para') ]] Ref N° [[ o.sale_id.name ]]</para>
    <para style="terp_header">[[ o.sale_id.state in ['progress','manual','done','invoice_except','shipping_except'] and removeParentNode('para') ]] Disclaimer: Not Confirmed Document </para>
    <para style="terp_default_8">
      <font color="white"> </font>
    </para>
    <blockTable colWidths="132.0,134.0,135.0,135.0" style="Table2">
      <tr>
        <td>
          <para style="terp_tblheader_General_Centre">[[ o.sale_id.state in ['draft','sent'] and removeParentNode('para') ]] Date</para>
          <para style="terp_tblheader_General_Centre">[[ o.sale_id.state not in ['draft','sent'] and removeParentNode('para') ]] Date</para>
        </td>
        <td>
          <para style="terp_tblheader_General_Centre">Sales Person</para>
        </td>
        <td>
          <para style="terp_tblheader_General_Centre">Price &amp; Payment Option</para>
        </td>
        <td>
          <para style="terp_tblheader_General_Centre">Remarks </para>
        </td>
      </tr>
    </blockTable>
    <blockTable colWidths="132.0,134.0,135.0,135.0" style="Table3">
      <tr>
        <td>
          <para style="terp_default_Centre_8">[[ formatLang(o.signup_date,date = True) ]]</para>
        </td>
        <td>
          <para style="terp_default_Centre_8">[[ (o.name and o.name) or '' ]]</para>
          <para style="terp_default_Centre_8">[[ format(o.saleperson and o.saleperson.name) or '' ]]</para>
        </td>
        <td>
          <para style="terp_default_Centre_8">[[ format(o.payment_option and o.payment_option.name) or '' ]]</para>
        </td>
        <td>
