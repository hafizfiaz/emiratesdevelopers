      <blockTable colWidths="80.0,83.0,56.0,59.0,64.0,60.0,65.0,70.0" style="Table5">
        <tr>
          <td>
            <para style="terp_default_9">[[ format(o.product_id.name) ]] </para>
          </td>
          <td>
            <para style="terp_default_9">[[ format(o.occupation_programe) ]] </para>
          </td>
          <td>
            <para style="terp_default_9">[[ format(o.occupation_no) ]] </para>
          </td>
          <td>
<para style="terp_default_Right_9">[[ formatLang(o.sale_id.order_line.product_uos and o.sale_id.order_line.product_uos_qty or o.sale_id.order_line.product_uom_qty) ]] [[ o.sale_id.order_line.product_uos and o.sale_id.order_line.product_uos.name or o.sale_id.order_line.product_uom.name ]]</para>
          </td>
          <td>
            <para style="terp_default_Right_9">[[ format(o.unit_price)]]</para>
          </td>
          <td>
            <para style="terp_default_Centre_9">[[ ', '.join(map(lambda x: x.name, o.sale_id.order_line.tax_id)) ]]</para>
          </td>
          <td>
            <para style="terp_default_Centre_9">[[show_discount(user.id) and formatLang(o.discount, digits=get_digits(dp='Discount')) or '']]</para>
          </td>
          <td>
            <para style="terp_default_Right_9">[[ format(o.price_subtotals)]] </para>
          </td>
        </tr>
      </blockTable>
    </section>
